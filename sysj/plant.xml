<System>
  <Interconnection>
    <Link Type="Destination">
      <!--Interface example: -->
      <!--<Interface SubSystem="SS1" Class="systemj.desktop.TCPIPInterface" Args="127.0.0.1:1100">-->
      <!--<Interface SubSystem="SS2" Class="systemj.desktop.TCPIPInterface" Args="127.0.0.1:1200">-->
      <!--.....-->
    </Link>
  </Interconnection>
  <SubSystem Name="SS2" Local="true">
    <ClockDomain Name="PlantCD" Class="Plant">
      <oSignal Name="pusherRetracted" To="ControllerCD.pusherRetracted" Class="systemj.signals.network.SimpleClient" IP="127.0.0.1" Port="10000"/>
      <oSignal Name="pusherExtended"  To="ControllerCD.pusherExtended"  Class="systemj.signals.network.SimpleClient" IP="127.0.0.1" Port="10000"/>
      <oSignal Name="WPgripped"       To="ControllerCD.WPgripped"       Class="systemj.signals.network.SimpleClient" IP="127.0.0.1" Port="10000"/>
      <oSignal Name="armAtSource"     To="ControllerCD.armAtSource"     Class="systemj.signals.network.SimpleClient" IP="127.0.0.1" Port="10000"/>
      <oSignal Name="armAtDest"       To="ControllerCD.armAtDest"       Class="systemj.signals.network.SimpleClient" IP="127.0.0.1" Port="10000"/>
      <oSignal Name="empty"           To="ControllerCD.empty"           Class="systemj.signals.network.SimpleClient" IP="127.0.0.1" Port="10000"/>

      <iSignal Name="pusherExtend"  Class="systemj.signals.network.SimpleServer" IP="127.0.0.1" Port="10001" />
      <iSignal Name="vacOn"         Class="systemj.signals.network.SimpleServer" IP="127.0.0.1" Port="10001" />
      <iSignal Name="armSource"     Class="systemj.signals.network.SimpleServer" IP="127.0.0.1" Port="10001" />
      <iSignal Name="armDest"       Class="systemj.signals.network.SimpleServer" IP="127.0.0.1" Port="10001" />

			<!-- From viz -->
      <iSignal Name="enable"        Class="systemj.signals.network.SimpleServer" IP="127.0.0.1" Port="10001" />
      <iSignal Name="refill"        Class="systemj.signals.network.SimpleServer" IP="127.0.0.1" Port="10001" />

			<!-- To viz-->
      <oSignal Name="pusherRetractedE" To="UI.pusherRetractedE" Class="systemj.signals.network.SimpleClient" IP="127.0.0.1" Port="20000"/>
      <oSignal Name="pusherExtendedE"  To="UI.pusherExtendedE"  Class="systemj.signals.network.SimpleClient" IP="127.0.0.1" Port="20000"/>
      <oSignal Name="WPgrippedE"       To="UI.WPgrippedE"       Class="systemj.signals.network.SimpleClient" IP="127.0.0.1" Port="20000"/>
      <oSignal Name="armAtSourceE"     To="UI.armAtSourceE"     Class="systemj.signals.network.SimpleClient" IP="127.0.0.1" Port="20000"/>
      <oSignal Name="armAtDestE"       To="UI.armAtDestE"       Class="systemj.signals.network.SimpleClient" IP="127.0.0.1" Port="20000"/>
      <oSignal Name="emptyE"           To="UI.emptyE"           Class="systemj.signals.network.SimpleClient" IP="127.0.0.1" Port="20000"/>
			<!-- viz end -->
    </ClockDomain>
  </SubSystem>
</System>
